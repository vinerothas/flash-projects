package{
	
	public class Building{
		
		public var stone:int;
		public var wood:int;
		public var money:int;
		public var time:int;
		public var builders:int;
		public var buildingName:String;
		
		public function Building(stone:int,wood:int,money:int,time:int,builders:int,buildingName:String){
			this.stone = stone;
			this.wood = wood;
			this.money = money;
			this.time = time;
			this.builders = builders;
			this.buildingName = buildingName;
		}
		
	}

	
}